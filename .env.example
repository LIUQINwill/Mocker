# 数据库配置
DATABASE_URL=mysql+pymysql://root:mocker123@mysql:3306/mocker
DATABASE_ECHO=false

# 应用配置
APP_NAME=Mocker API Platform
APP_VERSION=0.1.0
DEBUG=false
API_V1_STR=/api/v1
MOCK_PREFIX=/mock

# CORS配置
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8080","http://localhost:5173","http://localhost:80"]

